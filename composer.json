{
    "name": "miladghofrani/DecoratorBundle",
    "description": "Decode, transform and decorate third-party api response",
    "type": "symfony-bundle",
    "license": "proprietary",
    "authors": [
        {
            "name": "Milad Ghofrani",
            "email": "milad.ghofrani@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "miladghofrani\\Decorator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "miladghofrani\\Decorator\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=7.4",
        "symfony/framework-bundle": "^4.0 | ^5.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^9.5",
        "symfony/yaml": "^5.3",
        "symfony/serializer": "^5.3"
    },
    "conflict": {
        "doctrine/orm": "<2.7",
        "symfony/http-foundation": "<4.4",
        "symfony/framework-bundle": "<4.4"
    }
}
